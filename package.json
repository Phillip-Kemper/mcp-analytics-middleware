{
  "name": "mcp-analytics-middleware",
  "version": "1.0.0",
  "description": "Analytics middleware for Model Context Protocol servers",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cd web-viewer && yarn install && yarn build",
    "view-analytics": "node dist/view-analytics.js --db-path=analytics.db",
    "inspector": "yarn run build && npx @modelcontextprotocol/inspector node dist/server.js --analytics --db-path=analytics.db",
    "server": "node dist/server.js --analytics --db-path=analytics.db",
    "web-viewer": "node dist/web-viewer/server.js --db-path=analytics.db",
    "prepublishOnly": "yarn run build"
  },
  "bin": {
    "mcp-analytics-view": "./dist/view-analytics.js"
  },
  "keywords": [
    "mcp",
    "analytics",
    "middleware",
    "ai",
    "llm"
  ],
  "author": "Phillip Kemper",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phillip-Kemper/mcp-analytics-middleware"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "better-sqlite3": "^8.6.0",
    "zod": "^3.22.4",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9"
  }
}
